bilang main() {
    bilang start;
    bilang end;
    ilimbag("Enter start year:");
    start = ipasok();
    ilimbag("Enter end year:");
    end = ipasok();

    bilang temp = start - ((start / 4) * 4);
    bilang leap = (start / 4) * 4;

    if ( temp == 0 ) {
        ilimbag(leap);
    }

    leap = leap + 4;

    habang ( leap <= end) {
        ilimbag(leap);
        leap = leap + 4;
    }

    ibalik 0;
}